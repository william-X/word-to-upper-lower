{
  "transform": {
    "^.+\\.js$": "babel-jest",
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "transformIgnorePatterns": [
    "<rootDir>/node_modules/(?!(lodash-es|other-es-lib))"
  ]
}